alias l="ls -lah"

# GIT
alias gs="git status"
alias gd="git diff"
alias gc="git commit"
alias ga="git add"
alias gp="git push"
alias gpush="git push"
alias gfpush="git push --force-with-lease"
alias gpull="git pull"
alias gl="git log"
alias gco="git checkout"
alias gcb="git checkout -b"
alias gbd="git branch -D"
alias gfetch="git fetch -p"
alias gbowners="git for-each-ref --format='%(committerdate)%09%(authorname)%09%(refname)' | sort -k5n -k2M -k3n -k4n | grep remotes | awk -F \"\t\" '{ printf \"%-32s %-27s %s\n\", \$1, \$2, \$3 }'"
alias ggone="git branch -vv | awk '/: gone]/{print $1}'"
alias gstash="git stash save -u"
