alias l="ls -lah"

# GIT
alias gs="git status"
alias gd="git diff"
alias gc="git commit"
alias ga="git add"
alias gbowners="git for-each-ref --format='%(committerdate)%09%(authorname)%09%(refname)' | sort -k5n -k2M -k3n -k4n | grep remotes | awk -F \"\t\" '{ printf \"%-32s %-27s %s\n\", \$1, \$2, \$3 }'"
